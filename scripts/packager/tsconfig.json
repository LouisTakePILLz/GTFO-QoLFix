{
  "include": [ "src/**/*" ],
  "exclude": [],
  "compilerOptions": {
    "rootDir": "",
    "outDir": "dist",
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "types": [ "node" ]
  }
}
